type Query {
    shows(titleFilter: String): [Show]
    bars(foo: Int): Foo
}


type Mutation {
   createShow(showInput: ShowInput!): Show
}

type Show {
    id: Int!
    title: String
    releaseYear: Int
}

input ShowInput {
    title: String!
    releaseYear: Int!
}

enum NumberEnum {
  ONE,
  TWO
}

type Foo {
    id: Int
    bar: Boolean!
}
